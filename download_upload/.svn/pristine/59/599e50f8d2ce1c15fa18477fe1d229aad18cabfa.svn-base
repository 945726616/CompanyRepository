        function ccm_message_search()
        {

            var node, node_sn, pnode, pnode_sn,
                filter_list, filter_str = "", start_point, end_point, tz_offset;

            log_index = 1;
            node_sn = g_ref.select_device_ipc;
            node = g_ref.device_list_obj["sn_" + node_sn];
            if(!node)
            {
               // log("Please select the specific device in ccm_message_search.");
                return;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }

            filter_list = createLinkList();
            if(checkbox_config.checked)
                filter_list.add("config");

            if(checkbox_alarm.checked)
            {
                filter_list.add("sdcard");
                filter_list.add("reboot");
                filter_list.add("upgrade");
                filter_list.add("device");
                filter_list.add("motion");
            }
            else
            {
                if(checkbox_SD_card.checked)
                    filter_list.add("sdcard");
                if(checkbox_reboot.checked)
                    filter_list.add("reboot");
                if(checkbox_upgrade.checked)
                    filter_list.add("upgrade");
                if(checkbox_device_status.checked)
                    filter_list.add("device");
                if(checkbox_motion_detection.checked)
                    filter_list.add("motion");
            }

            if(checkbox_operate.checked)
            {
                filter_list.add("talk");
                filter_list.add("record");
                filter_list.add("play");
            }
            else
            {
                if(checkbox_talk.checked)
                    filter_list.add("talk");
                if(checkbox_record.checked)
                    filter_list.add("record");
                if(checkbox_play.checked)
                    filter_list.add("play");
            }

            for(var filter_ptr = filter_list.get_first(); filter_ptr; filter_ptr = filter_ptr.next)
            {
                filter_str += filter_ptr.data;
                if(filter_ptr.next)
                    filter_str += "|"
            }

            tz_offset = 0;
            if (($.browser.msie || $.browser.chrome))
            {
                tz_offset = (new Date()).getTimezoneOffset() * 60;
            }

            start_point = mx("#input_begin_time").value == mcs_begin_time? "" : mx("#input_begin_time").value.replace(/ /, "T") + ":00";
            end_point = mx("#input_end_time").value == mcs_end_time? "" : mx("#input_end_time").value.replace(/ /, "T") + ":00";
            start_point = start_point ? (Date.parse(start_point) / 1000 + tz_offset) : "";
            end_point = end_point ? (Date.parse(end_point) / 1000 + tz_offset) : "";


            acs.send_msg("ccm_msg_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn},
                flag:0, start_time:start_point, end_time:end_point, filter:filter_str, start:search_time, counts:get_event_count},
                function(type, msg, ref){ccm_message_search_ack(type, msg, ref);}, {ip:pnode.ip, to:"ccm", sn:node_sn, psn:pnode_sn});
        }
