        function set_time_func(obj)
        {
            var l = obj.l, t = obj.t, p = obj.p,
                move_dist = ptz_speed * 4 / 10;

            while(p !== null)
            {
                l += p.offsetLeft;
                t += p.offsetTop;
                p = p.offsetParent;
            }

            var R = 62, r = 18;
            var origin_x = l;
            var origin_y = t;
            var center_x = origin_x + 62;
            var center_y = origin_y + 62;
            var mou_x = obj.mou_x;
            var mou_y = obj.mou_y;
            var ox = mou_x - origin_x;
            var oy = mou_y - origin_y;
            var cx = mou_x - center_x;
            var cy = mou_y - center_y;
            var xie = Math.sqrt(Math.pow(cx, 2) + Math.pow(cy, 2));

            if(xie > R)
            {
                return;
            }
            else if(xie < r)
            {
            }
            else
            {
                if(cx < 0 && cy < 0)              //第二象限
                {
                    if(ox < oy)                   //left
                    {
                        if(flag == "stop")
                        {
                            parent_callback({type:"relative_move",
                                data:{pantilt:{x:-1, y:0, space:"http://www.onvif.org/ver10/tptz/PanTiltSpaces/SphericalTranslatcionSpace Degrees"},
                                    zoom:{x:0, space:""},
                                speed:{pantilt:{x:5, y:5, space:""}, zoom:{x:0, space:""}}}});
                        }
                        else
                        {
                            if(interval)
                            {
                                clearInterval(interval);
                                mark.flag = "ready";
                            }
                            parent_callback({type:"relative_move",
                                data:{pantilt:{x:-move_dist, y:0, space:"http://www.onvif.org/ver10/tptz/PanTiltSpaces/SphericalTranslationSpace Degrees"},
                                    zoom:{x:0, space:""},
                                speed:{pantilt:{x:ptz_speed, y:ptz_speed, space:""}, zoom:{x:0, space:""}}, mark:mark}});
                            interval = setInterval(function(){
                                if(mark.flag == "ready")
                            {
