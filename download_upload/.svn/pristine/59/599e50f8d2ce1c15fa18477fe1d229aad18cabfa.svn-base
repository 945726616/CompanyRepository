               function ccm_get_network_info_response(type, msg, ref)
               {
               	    if(msg && msg.ret&&msg.ret.code == "")
                    {
                    	  if(msg.info && msg.info.ifs )
                    	  {
                    	      for(var i=0;i<msg.info.ifs.length;i++)
                    	      {
                    	          if(msg.info.ifs[i].token=="ra0")
                    	          {
								     if( msg.info.ifs[i].enabled)
									 {
                    	          	    now_ifs=msg.info.ifs[i];
                    	             	if(msg.info.ifs[i].enabled==1 && msg.info.ifs[i].wifi_client && msg.info.ifs[i].wifi_client.info.stat=="ok")
                    	             	{
                    	                  device_con[s_innerHTML]="";
                    	                  if(obj.old_page=="set_pass" &&set_wifi_con==1)
                    	                  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:3});
                    	                  }
                    	                  else if(obj.old_page=="add_device" &&set_wifi_con==1)
                    	                  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:4});
                    	                  }
                    	                  else if(obj.old_page=="set_pass" && set_wifi_con==0)
                    	                  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:2});
                    	                  }
                    	                  else 
                    	                  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:1});
                    	                  }
                    	             	}
										else if(msg.info.ifs[i].phy&& msg.info.ifs[i].phy.info && msg.info.ifs[i].phy.info.status=="err" && set_wifi_con==0)
										{
										   if(obj.old_page=="add_device")
										  {
										     net_cable_page({option:"is_configured_wifi",Completion_status:1});
										  }else if(obj.old_page=="set_pass")
										  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:2});
										  }
										}
                    	             	else if(msg.info.ifs[i].wifi_client && !set_wifi_con)
                    	                {
										   if( msg.info.ifs[i].wifi_client.ap_list )
										   {
                    	                  var iner_htm="";
                    	              	  for(var j=0;j<msg.info.ifs[i].wifi_client.ap_list.length;j++)
                    	              	  {
                    	              	     iner_htm+="<option style='color:white;' value='"+msg.info.ifs[i].wifi_client.ap_list[j].ssid+"'>"+msg.info.ifs[i].wifi_client.ap_list[j].ssid+"</option>"	;
                    	              	 }
                    	              	 
										  $("#select_ssid").html(iner_htm);
										   }
                                          $(me_parent).show();
										  $(select_ssid).tzSelect(null,2);
										  
										  device_con[s_innerHTML]="";
                    	                }
                    	                else if(set_wifi_con==1)
                    	                {
										    if(!me_parent) return;
											setTimeout(function(){
                    	              		ccm_get_network_info_request("all");},3000);
                    	                }
										
								    }
									else
										{
										  if(obj.old_page=="add_device")
										  {
										     net_cable_page({option:"is_configured_wifi",Completion_status:1});
										  }else if(obj.old_page=="set_pass")
										  {
                    	                      net_cable_page({option:"is_configured_wifi",Completion_status:2});
										  }
									    }
                    	          }
                    	      }	
                    	  }
                    }
