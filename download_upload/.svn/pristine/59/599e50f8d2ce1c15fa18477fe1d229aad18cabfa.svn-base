            function ccm_add_sub_device_response(type, msg, ref)
            {
                   if(!me_parent) return;
                   if(msg && msg.ret&& msg.ret.code == "")
                   {
              	      add_device(msg.info, ref.sn);
                    pos = g_ref.device_list_link.pos("sn_" + msg.info.sn);
              	    if(g_ref.device_list_obj["sn_" + msg.info.sn].state.online == "Online")
                    {
                  	      $(".device_list_li_span:eq(" + pos + ")").click();
                  	}
                  	  CcmDiskCtrlRequest({sn:msg.info.sn,pwd:ref.pwd});
            		}
            		else if(msg.ret.sub)
            		{
