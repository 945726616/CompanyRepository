            }
            acs.send_msg("ccm_upgrade_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn},
                check:(obj && obj.check)?obj.check:0},
                    function(type, msg, ref){ccm_get_upgrade_status_response(type, msg, ref);},
                    {ip:pnode.ip, to:"ccm", sn:node_sn, psn:pnode_sn, check_ver:(obj && obj.check)?obj.check:0, type:(obj && obj.type)?obj.type:0});
 
        }
        function ccm_get_upgrade_status_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                var msg_status = msg.task?msg.task.stat:msg.stat;
                
                if( ref.check_ver && msg_status == "fail" )
                {
                   msg_status="free";
                }
                  
                if(ref.type == "online" && msg_status == "")
                {
                    system_upgrade_div[s_innerHTML] = mcs_already_latest_version;
                }
                else if(msg_status == "fail" )
                {
                    system_pop_confirm_box({alert:true, str:mcs_fail + ":" +　msg.task.remark});
                }
                else if(msg_status != "free" && msg_status != "" && msg_status != "finish" )
                {
                    var wait_div = mx("#system_wait_div"), str_div = mx("#cl_str_div"),
                        i, extra = "";

                    setTimeout(function(){ccm_get_upgrade_status_request();}, 1000);
                    if(ref.type == "online")
                        system_upgrade_div[s_innerHTML] = mcs_upgrading + "...";
                    if(!wait_div) g_ref.system_wait_div("");
                    else
                    {
                        for(i = 1; i <= extra_timer; ++i)
                        {
                            extra += "."
                        }
                        extra_timer = ++extra_timer > 3 ? 1 : extra_timer;
                        if(msg_status == "download")
                        {
                            str_div[s_innerHTML] = mcs_downloading+ extra;
                        }
                        else if(msg_status == "erase")
                        {
                            str_div[s_innerHTML] = mcs_erasing + extra;
                        }
                        else if(msg_status == "write")
                        {
                            str_div[s_innerHTML] =  mcs_writing + " " + msg.progress + "%";
                        }
                    }
                }
                else
                {
                    g_ref.system_stop_wait();
                    extra_timer = 1;
                    if(ref.check_ver)
                    {
                        if(msg._cur_ver < "12.09.13.04.04")
                        {
                            system_upgrade_div.parentNode[s_style].display = "none";
                        }
                        if(msg._valid_ver == msg._cur_ver)
                        {
                            system_upgrade_left[s_innerHTML]= mcs_online_upgrade;
                            system_upgrade_div[s_innerHTML] = mcs_already_latest_version;
                        }
                        else 
                        {
                            system_upgrade_left[s_innerHTML]= mcs_online_upgrade;
                            system_upgrade_div[s_innerHTML] = mcs_new_version+msg._valid_ver+mcs_valid + "&nbsp;<button class='standard_buttons_white'>" + mcs_upgrade + "</button>";
                            mx("/button", system_upgrade_div)[0].onclick = function(){
                                system_pop_confirm_box({str:mcs_upgrade_to_the_latest_version+msg._valid_ver,
                                    callback_func:function(obj){ccm_upgrade_request({firmware_src:"download"});}
                                })
                            }
                        }
                    }
                    else
                    {
                        system_pop_confirm_box({str:mcs_upgrade_successful_restart_to_take_effect, button_left:mcs_reset_now, button_right:mcs_reset_later,
                            callback_func:function(obj){system_reboot_request(obj);}})
                    }
                }
            }
            else
            {
                log("error:ccm_upgrade_get_ack return Result error in class_options_system_maintenance.");
                return -1;
            }
        }
