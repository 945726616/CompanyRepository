        function ccm_get_recording_task_config_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                if(ref.type == "setup")
                {
                    create_control_recording_setup({task_config:msg.task});
                }
                
            }
        }
        function input_time_change(obj)
        {
           if(obj.type=="string")
           {
       
            return  obj.data.substring(obj.data.indexOf(":"),0)*60*60+obj.data.substring(obj.data.indexOf(":")+1,obj.data.length)*60;
           }
           else if(obj.type=="int")
           {
              var hour,Minute;
              hour=parseInt(obj.data/3600);
              Minute=(obj.data%3600)/60;
              if(hour<10)
              {
                hour="0"+hour;
              }
              if(Minute<10)
              {
                Minute="0"+Minute;
              }
              return hour+":"+Minute;
           }  
        }
        function create_control_recording_setup(obj)
        {
           
           if(obj.task_config)  
           {
             task_config=obj.task_config;
             if(task_config.sch)
             {
                if(task_config.sch.enable)
                {
                    $("#checkbox_schedule").iButton("toggle",true);
                }else
                {
                   $("#checkbox_schedule").iButton("toggle",false); 
                }
                if(task_config.sch.full_time)
                {
                    $(radio_H7_24).click();
                    $(radio_schedule_content).hide();
                }
                else if(task_config.sch.times)
                {
                    $(radio_schedule).click();
                    $(radio_schedule_content).show();
                    for(i=0;i<task_config.sch.times.length;i++)
                    {
                       if(task_config.sch.times[i].wday)
                       {
                            $("#schedule_record"+(i+1)).iButton("toggle",true);
                            week_checkbox_class=mx(".week_checkbox_class"+(i+1));
                            
                            mx("#input_begin_time"+(i+1)).value=input_time_change({type:"int",data:task_config.sch.times[i].start});
                            mx("#input_end_time"+(i+1)).value=input_time_change({type:"int",data:task_config.sch.times[i].end});
                            for(j=0;j<week_array.length;j++)
                            {
                                if(task_config.sch.times[i].wday & week_array[j])
                                {
                                   for(k=0;k<week_checkbox_class.length;k++)
                                   {
                                      if(week_checkbox_class[k].value==j)
                                      {
                                        week_checkbox_class[k].checked=true;
                                      }
                                   }
                                }
                            }
                       }
                       else
                        {
                            $("#schedule_record"+(i+1)).iButton("toggle",false);
                       }
                    }
                }
             }    
           }
            $("#manager_page").mCustomScrollbar("update");
              $(".week_checkbox_div1").jNice();
            $(".week_checkbox_div2").jNice();
            $(".week_checkbox_div3").jNice();
            $(".week_checkbox_div4").jNice();
        }
