        function ccm_alertdevice_get_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
               
                if(msg.conf.io_in_mode=="Open")
                {
                    select_input_level.value=mcs_normally_open;
                }
                else
                {
                	 select_input_level.value=mcs_normally_close;
                }
                if(msg.conf.io_out_mode=="Open")
                {
                    select_linkage_level.value=mcs_normally_open;
                }
                else
                {
                	 select_linkage_level.value=mcs_normally_close;
                }

                

                MotionDebounceTime=msg.conf.motion_debounce_time;
                PirDebounceTime= msg.conf.pir_debounce_time;
                $("#input_thresholdLevelNight").addClass("fd_range_" + 0 + "_" + 100);
                $("#input_threshold").addClass("fd_range_" + 0 + "_" + 100); 
                    fdSliderController.create();
                if(mx("#input_thresholdLevelNight"))
                    fdSliderController.increment("input_thresholdLevelNight", msg.conf.motion_level_night - mx("#input_thresholdLevelNight").value );
                if(mx("#input_threshold"))
                    fdSliderController.increment("input_threshold", msg.conf.motion_level - mx("#input_threshold").value );
            }
            else
            {
                log("error:ccm_alertdevice_get_response return Result error in class_options_alarm.");
                return -1;
            }
        }
