        function refresh_change(obj)
        {
            var select_ipc = g_ref.device_list_obj["sn_" + g_ref.select_device_ipc];
            if(g_ref.now_page == "home")
            {
                parent_callback({type:"delete_ipc", data:{node_sn:obj.old_sn, type:"refresh"}});
                if(select_ipc.state.view)
                    parent_callback({type:"create_ipc", data:{node_sn:g_ref.select_device_ipc}});
            }
            else if(g_ref.now_page == "playback")
            {
                parent_callback({type:"delete_ipc", data:{node_sn:obj.old_sn, type:"refresh"}});
                if(select_ipc.state.playback)
                    parent_callback({type:"create_ipc", data:{node_sn:g_ref.select_device_ipc}});
            }
            return 0;
        }
        function split_screen(obj)
        {
            var count = 0, old_count;

            if(g_ref.now_page == "home")
            {
                old_count = g_ref.view_split_count;
                count = g_ref.view_split_count;
                if(obj.count)
                {
                    count = g_ref.view_split_count = obj.count;
                }
            }
            else if(g_ref.now_page == "playback")
            {
                old_count = g_ref.playback_split_count;
                count = g_ref.playback_split_count;
                if(obj.count)
                {
                    count = g_ref.playback_split_count = obj.count;
                }
            }

            while(count--)
            {
                inner_windows = dom_create_child(inner_windows_cont, "div", "", "inner_windows");
            }
            adjust_screen(obj);
            if(obj.type && obj.type == "create")
                fill_inner_ipc({count:obj.count, old_count:old_count, type:"not_ipc"});
            else
                fill_inner_ipc({count:obj.count, old_count:old_count, type:"add_ipc"});
        }
        function adjust_screen(obj)
        {
            var split_count,
                count,
                inner_window_w,
                inner_window_h;

            mx("#inner_windows_cont").style.width = g_ref.main_window_w + "px";
            mx("#inner_windows_cont").style.height = g_ref.main_window_h + "px";
            if(g_ref.now_page == "home")
            {
                split_count = count = g_ref.view_split_count;
            }
            else if(g_ref.now_page == "playback")
            {
                split_count = count = g_ref.playback_split_count;
            }

            if(4 == split_count || 9 == split_count || 16 == split_count || 36 == split_count)
            {
                inner_window_w = Math.floor(g_ref.main_window_w / Math.sqrt(split_count));
                inner_window_h = Math.floor(g_ref.main_window_h / Math.sqrt(split_count));
                while(count--)
                    mx(".inner_windows")[count][s_style][s_cssText] = "float:left;width:" + inner_window_w + "px;height:" + inner_window_h + "px;";
            }
            else if(8 == split_count)
            {
                inner_window_w = Math.floor(g_ref.main_window_w / 4);
                inner_window_h = Math.floor(g_ref.main_window_h / 4);

                while(count--)
                    mx(".inner_windows")[count][s_style][s_cssText] = "float:left;width:" + inner_window_w + "px;height:" + inner_window_h + "px;";
                mx(".inner_windows")[0][s_style][s_cssText] = "float:left;width:" + (inner_window_w * 3) + "px;height:" + (inner_window_h * 3) + "px;";
            }
            //The wrong data also use 1 split_screen;
            else
            {
                inner_window_w = g_ref.main_window_w;
                inner_window_h = g_ref.main_window_h;
                mx(".inner_windows")[0].style.width = inner_window_w + "px";
                mx(".inner_windows")[0].style.height = inner_window_h + "px";
            }
        }
        function fill_inner_ipc(obj)
        {
            var i = 1, count, id_count,
                array_count = 0,
                list_length,
                list_obj,
                inner_windows = mx(".inner_windows"),
                inner_window_obj;

            if(g_ref.now_page == "home")
            {
                count = obj.old_count;
                id_count = (g_ref.now_view_page - 1) * count + 1;
                list_length = g_ref.view_link.length;
                list_obj = g_ref.view_link.get_first();
                if(obj.count)
                {
                    count = obj.count;
                    g_ref.now_view_page = Math.ceil(id_count / obj.count);
                    id_count = (g_ref.now_view_page - 1) * count + 1;
                }
            }
            else if(g_ref.now_page == "playback")
            {
                count = obj.old_count;
                id_count = (g_ref.now_playback_page - 1) * count + 1;
                list_length = g_ref.playback_link.length;
                list_obj = g_ref.playback_link.get_first();
                if(obj.count)
                {
                    count = obj.count;
                    g_ref.now_playback_page = Math.ceil(id_count / obj.count);
                    id_count = (g_ref.now_playback_page - 1) * count + 1;
                }
            }

            g_ref.window_array = new Array(count);

            while(i++ < id_count)
                list_obj = list_obj.next;
            while(count--)
            {
                window_array_info["inner_window" + id_count] = {dom_id:("inner_window" + id_count), index:id_count, video_chls:null, audio_chls:null, mme:null, ipc_state:"", node_sn:""};
                inner_window_obj = new class_inner_window({parent:inner_windows[array_count], inner_window_info:window_array_info["inner_window" + id_count],
                    on_event:function(obj){on_event(obj);}});
                window_array_info["inner_window" + id_count].inner_obj = inner_window_obj;
                if(id_count <= list_length && obj.type == "add_ipc")
                {
                    if(g_ref.now_page == "home")
                        add_home_ipc({inner_window_info:window_array_info["inner_window" + id_count], node_sn:list_obj.data});
                    else if(g_ref.now_page == "playback")
                        add_playback_ipc({inner_window_info:window_array_info["inner_window" + id_count], node_sn:list_obj.data});
                    list_obj = list_obj.next;
                }
                if(g_ref.now_page == "home")
                {
                   me.ctrl({type:"snapshot",data:1});
                }
                ++id_count;
                ++array_count;
