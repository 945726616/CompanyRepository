                  if(Duplicate_send_msg_ex.data && Duplicate_send_msg_ex.data.sess)
                  {
                       data.sess={nid:acs.create_nid(acs.ctrl({type:"get_info"})),sn:g_ref.select_device_ipc};
                  }else if(Duplicate_send_msg_ex.data && Duplicate_send_msg_ex.data.nid)
                  {
                       data.Nfid=acs.create_nid(acs.ctrl({type:"get_info"}));    
                  } 
                  me.send_msg(Duplicate_send_msg_ex.type,data , Duplicate_send_msg_ex.on_ack, Duplicate_send_msg_ex.ref);
                }
                else
                {
                   me.send_msg_ex(Duplicate_send_msg_ex.sn,Duplicate_send_msg_ex.type, Duplicate_send_msg_ex.data, Duplicate_send_msg_ex.on_ack, Duplicate_send_msg_ex.ref);
                }
        }
        function on_cacs_logout_ack(type, msg, ref)
        {
            if(msg.result == ""||g_ref.service_options=="IPC"||g_ref.service_options=="BOX")
            {
                var remember_data = get_storage_data({str:str.remember_msg_str});
                local_storage.set(str.remember_msg_str, "");
            }
            else
            {
                log("error:cacs_logout_ack return result error in class_account.");
            }
            ms.ctrl({type:"create_login_interface", data:{type:g_ref.service_options, sn:ref.sn}});
        }
        function on_event(obj)
        {
            switch(obj.type)
            {
                case "login_ack":
                {
                     on_cacs_login_ack(obj.data.type,obj.data.msg,obj.data.ref);
                     break;  
                     
                }
				case "Guide_ok":
				{
				    ms.ctrl({type:"ccm_get_device", data:{ip:g_ref.server_device}});
				    break;
				}
                case "connection_is_interrupted":
                {
                    system_pop_confirm_box({alert:true, str:mcs_connection_is_interrupted, callback_func:function(obj){window.location.reload(true);}});
                     break;    
                }
            }
        }
        me.ctrl = function(obj)//class_account 
        {
            switch(obj.type)
            {
                case "begin":
                    {
                        var remember_data = get_storage_data({str:str.remember_msg_str});
                        if(remember_data)
                        {
                            var reg = /^\d/;
                            if(reg.exec(remember_data.user))
                                g_ref.login_status = "ipc";
                            else
                                g_ref.login_status = "register_user";
                            now_handle_info.lid       = remember_data.lid;
                            now_handle_info.share_key = remember_data.share_key;
                            parent_callback({type:"ready"});
                            now_handle_info.share_key=null;
                           
                        }
                        else
                        {
                            parent_callback({type:"ready"});
                        }
                        break;
                    }
                case "get_info":
                    {
                        return now_handle_info;
                    }
                case "register":
                    {
                        cacs_reg_req(obj.data);
                        return;
                    }
                case "login":
                    {
                       //  login_dom.ctrl(obj);
					             now_handle_info       = {tid:0, lid:0, sid:0, seq:0, share_key:"", addr:""};
                       cacs_login_req(obj.data);
                       break;
                    }
                case "reset":
                    {
                         var secret_key = dh.gen_private();
                         var pub_key = dh.gen_public(secret_key);
                         var tid = now_handle_info.tid;
                         me.send_msg("cacs_dh_req", {bnum_prime:dh.prime, root_num:dh.g, key_a2b:pub_key, tid:tid},
                         function(type, msg, ref) {
                           if(msg.result == "")
                           {
                             var share_key = dh.gen_shared_secret(secret_key, msg.key_b2a);
                             local_storage.set(str.cacs_tid_str, msg.tid);
                             now_handle_info.tid       = msg.tid;
                             now_handle_info.lid       = msg.lid;
                             now_handle_info.share_key = share_key;
                             me.send_msg("cacs_recovery_req", {nid:acs.create_nid_base_lid(now_handle_info), user:obj.data.user,lang:g_ref.now_lang,param:[{name:"appid",value:g_ref.hostname}]},
                               function(type, msg, ref){
                                  if(msg.result =="")
                                  {
                                     system_pop_confirm_box({alert:true,str:mcs_password_reset_confirmation, callback_func:function(obj){}});      
                                  }
                                  else if(msg.result=="accounts.mail.invalid")
                                  {
                                     system_pop_confirm_box({alert:true,str:mcs_recovery_fail_no_mail, callback_func:function(obj){}}); 
                                  }
                                  else if(msg.result==err_cacs_user_unknown_0)
                                  {
                                     system_pop_confirm_box({alert:true,str:mcs_username_does_not_exis, callback_func:function(obj){}}); 
                                  }
                               },
                               {ip:ref.ip, to:"ccm"});
                           }
                         },
                        {ip:g_ref.server_device, to:"ccm"});
                        break;    
                    }
                case "reset_login":
                    {
                        var secret_key = dh.gen_private();
                         var pub_key = dh.gen_public(secret_key);
                         var tid = now_handle_info.tid;
                         me.send_msg("cacs_dh_req", {bnum_prime:dh.prime, root_num:dh.g, key_a2b:pub_key, tid:tid},
                         function(type, msg, ref) {
                           if(msg.result == "")
                           {
                             var share_key = dh.gen_shared_secret(secret_key, msg.key_b2a);
                             local_storage.set(str.cacs_tid_str, msg.tid);
                             now_handle_info.tid       = msg.tid;
                             now_handle_info.lid       = msg.lid;
                             now_handle_info.share_key = share_key;
                             
                             me.send_msg("cacs_passwd_req", {nid:acs.create_nid_base_lid(now_handle_info),
                                 user:obj.data.user,
                                 old_pass:me.ctrl({type:"md5_pass", data:{pwd:obj.data.old_pass}}),
                                 new_pass:me.ctrl({type:"md5_pass", data:{pwd:obj.data.pass}}),guest:0,param:[{name:"appid",value:g_ref.hostname}]},
                                 function(type, msg, ref){
                                      if(msg.result =="")
                                      {
                                           system_pop_confirm_box({str:mcs_recovery_ok_login,
                                               callback_func:function(){cacs_login_req({user:obj.data.user, password:obj.data.pass, flag:0}); temp_password = null;}});
                                      }
                                      else
                                      {
                                           system_pop_confirm_box({alert:true,str:mcs_recovery_fail_invalid_link,
                                               callback_func:function(){}});
                                      }
                                 }, {ip:g_ref.server_device, to:"ccm"});
                           }
                         },
                          {ip:g_ref.server_device, to:"ccm"});
                        break;
                    }
                case "logout":
                    {
                        me.send_msg("cacs_logout_req", {nid:acs.create_nid(acs.ctrl({type:"get_info"}))},
                        function(type, msg, ref){on_cacs_logout_ack(type, msg, ref);}, {ip:g_ref.server_device, to:"ccm", type:obj.data.type, sn:obj.data.sn});
                        break;
                    }
                case "md5_pass":
                    {
                       return des_md5_encrypt(md5_message_encrypt(obj.data.pwd), now_handle_info.share_key);
                    }
			    case "md5_message_encrypt":
				    {
					   return md5_message_encrypt(obj.data.pwd);
					}
                case "ready_login":
                    {
                        return auto_login || ready_login;
                    }
                case "Duplicate_send_msg":
                    {
                       Duplicate_send_msg();
                       break; 
                    }
			
                default:
                    log("error:ctrl's obj.type is not exist in class_account.");
            }
        }
        me.send_msg = function(type,data, on_ack, ref)
        {
            if(type != "mmq_pick")
                system_signal_trans.create(type);
            rpc.call({srv:window.location.protocol + "//" + ref.ip + "/", to:ref.to, type:type, data:data, ref:ref, "static":false, way:"json", qid:(ref.qid == null)?null:ref.qid,
                on_ack:function(msg, ref) {
                    if(type != "mmq_pick")
                        system_signal_trans.destroy(type);
                        
                    if(msg && msg.data && msg.data.ret && msg.data.ret.sub && type != "mmq_pick")
                    {
                        if(msg.data.ret.reason == "InvalidSession" || msg.data.ret.sub=="accounts.sid.invalid" || msg.data.ret.sub=="accounts.nid.invalid" || msg.data.ret.sub=="accounts.lid.invalid" || msg.data.ret.sub=="ccms.session.invalid")
                        {
                            if(msg.from_handle<last_login.hfrom_handle)
                            {
                                if(data.sess)
                                {
                                    data.sess={nid:acs.create_nid(acs.ctrl({type:"get_info"}))};
                                }
                                if(data.nid)
                                {
                                    data.nid=acs.create_nid(acs.ctrl({type:"get_info"})); 
                                }
                                acs.send_msg(type,data,on_ack,ref);
                            }
                            else if((deteformat(new Date())-last_login.date)>5)
                            {
                                  if(type!="cacs_login_req" && type!="cacs_dh_req" && type != "mmq_pick"&& type!="ccm_subscribe")
                                  {
                                       Duplicate_send_msg_ex={type:type,data:data,on_ack:on_ack,ref:ref,send_msg:1};
                                  }
                                  ms.ctrl({type:"mmq_destroy"});
                                  setTimeout(function(){on_cacs_new_dh_req(temporary_account);},1000);
                            }
                            else if((deteformat(new Date())-last_login.date)<5)
                            {
                                 system_pop_confirm_box({alert:true, str:mcs_connection_is_interrupted, callback_func:function(obj){window.location.reload(true);}});      
                            }
                        }  
                        if(msg.data.ret.sub=="permission.denied" && type!="ccm_disk_ctl")
                        {
                           g_ref.system_prompt_box(mcs_permission_denied, 240, -200);
                           return -1;    
                        }
                    }
                    if(msg&& msg.data && (msg.data.result=="permission.denied" ||msg.data.ret=="permission.denied"))
                    {
                        g_ref.system_prompt_box(mcs_permission_denied);
                    }
                    if("object" == typeof(msg))
                    {
                        if(type=="cacs_login_req")
                        {
                           last_login.date = deteformat(new Date());
                           last_login.handle = msg.from_handle;
                        }
                        if(0 == system_exception_handling(msg.type, msg.data))
                            on_ack(msg.type, msg.data, ref);
                    }
                    else if("cancel" != msg)
                    {
                        if(0 == system_exception_handling(msg.type, msg.data))
                            on_ack(msg, msg, ref);
                    }
                }
            });
        }
        me.send_msg_ex = function(sn, type, data, on_ack, ref)
        {
            
            var node, node_sn, pnode, pnode_sn;
           
            node_sn = sn;
            node = g_ref.device_list_obj["sn_" + sn];
            if(!node)
            {
               
                return -1;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }
            data["sess"] = {nid:me.create_nid(now_handle_info), sn:node_sn};
			
            system_signal_trans.create(type); 
            
            rpc.call({srv:window.location.protocol + "//" + pnode.ip + "/", to:"ccm", type:type, data:data, ref:ref, "static":false, way:"json", qid:null,
                on_ack:function(msg, ref) {
                    system_signal_trans.destroy(type);
                    if(msg && msg.data && msg.data.ret && msg.data.ret.sub)
                    {
                       
                        if(msg.data.ret.reason == "InvalidSession" || msg.data.ret.sub=="accounts.sid.invalid" || msg.data.ret.sub=="accounts.nid.invalid" || msg.data.ret.sub=="accounts.lid.invalid" || msg.data.ret.sub=="ccms.session.invalid")
                        {
                            if(msg.from_handle<last_login.hfrom_handle)
                            {
                                 acs.send_msg(type,data ,on_ack,ref);
                            }
                            else if((deteformat(new Date())-last_login.date)>5)
                            {
                                if(type!="cacs_login_req" && type!="cacs_dh_req"&& type!="ccm_subscribe")
                                {
                                     Duplicate_send_msg_ex={sn:sn,type:type,data:data,on_ack:on_ack,ref:ref,send_msg:0};
                                }
                                 ms.ctrl({type:"mmq_destroy"});
                                 setTimeout(function(){on_cacs_new_dh_req(temporary_account);},1000);
                            }
                            else if((deteformat(new Date())-last_login.date)<5)
                            {
                                 system_pop_confirm_box({alert:true, str:mcs_connection_is_interrupted, callback_func:function(obj){window.location.reload(true);}});      
                                 
                            }
                        }
                        if(msg.data.ret.sub=="permission.denied" && type!="ccm_disk_ctl")
                        {
                           g_ref.system_prompt_box(mcs_permission_denied);
                           return -1;    
                        }
                    }
                    if(msg&& msg.data && (msg.data.result=="permission.denied" ||msg.data.ret=="permission.denied"))
                    {
                        g_ref.system_prompt_box(mcs_permission_denied);
                    }
                    if("object" == typeof(msg))
                    {
                        if(0 == system_exception_handling(msg.type, msg.data))
                            on_ack(msg.type, msg.data, ref);
                    }
                    else if("cancel" != msg)
                    {
                        if(0 == system_exception_handling(msg.type, msg.data))
                            on_ack(msg, msg, ref);
                    }
                }
            });
        }
    }
    window.class_account = class_account;
