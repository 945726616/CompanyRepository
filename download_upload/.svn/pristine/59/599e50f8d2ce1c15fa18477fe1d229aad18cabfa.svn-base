        function adjust_imaging_setting_slider(obj)
        {
            var slider = mx(".fd_tween", mx("#ptz_control_body")), i, length;
            length = slider.length;
            video_sources_token=obj.video_sources_token;
            if(obj.imaging)
            {
                if(g_ref.now_page != "home")return;
                fdSliderController.increment("ptz_slider_Brightness",obj.imaging.brightness  - mx("#ptz_slider_Brightness").value);
                fdSliderController.increment("ptz_slider_Contrast", obj.imaging.contrast- mx("#ptz_slider_Contrast").value);
                fdSliderController.increment("ptz_slider_ColorSaturation",obj.imaging.color_saturation- mx("#ptz_slider_ColorSaturation").value );
                fdSliderController.increment("ptz_slider_Sharpness", obj.imaging.sharpness - mx("#ptz_slider_Sharpness").value);
                if(determine_the_version({type:"Positive",version:"13.12.10.23.27"}))
                {
                	$(mode_div).show();
                	$(mode_left).show();
                   if(obj.imaging.mode=="auto"||obj.imaging.mode=="")
                   {
                        mx("#select_mode")[0].selected = true;
                   }
                   else if(obj.imaging.mode=="day")
                   {
               	   
                       mx("#select_mode")[1].selected = true;
                   }
                   else if(obj.imaging.mode=="night")
                   {
               	    
                       mx("#select_mode")[2].selected = true;
                   }
                   $("#select_mode").tzSelect();
                }
                else
                {
                	$(mode_div).hide();
                	$(mode_left).hide();
                }
            }
            else
            {
                for(i = 0; i < length; ++i)
                {
                    fdSliderController.disableSlider(slider[i]);
                }
            }
        }
        function add_event(obj)
        {
            ptz_control.onmousemove = function(event) {
                var l = this.offsetLeft;
                var t = this.offsetTop;
                var p = this.offsetParent;
                while(p !== null)
                {
                    l += p.offsetLeft;
                    t += p.offsetTop;
                    p = p.offsetParent;
                }

                var evt = event ? event : (window.event ? window.event : null);
                var R = 62, r = 18;
                var origin_x = l;
                var origin_y = t;
                var center_x = origin_x + 62;
                var center_y = origin_y + 62;
                var mou_x = evt.pageX || evt.clientX + document.body.scrollLeft + document.documentElement.scrollLeft;
                var mou_y = evt.pageY || evt.clientY + document.body.scrollTop + document.documentElement.scrollTop;
                var ox = mou_x - origin_x;
                var oy = mou_y - origin_y;
                var cx = mou_x - center_x;
                var cy = mou_y - center_y;
                var xie = Math.sqrt(Math.pow(cx, 2) + Math.pow(cy, 2));

                if(xie > R)
                {
                    background_img_change_pos(this, sub_img_PTZ0);
                }
                else if(xie < r)
                {
                    background_img_change_pos(this, sub_img_PTZ1);
                }
                else
                {
                    if(cx < 0 && cy < 0)                      //第二象限
                    {
                        if(ox < oy)                   //left
                        {
                            background_img_change_pos(this, sub_img_PTZ5);
                        }
                        else if(ox > oy)              //top
                        {
                            background_img_change_pos(this, sub_img_PTZ2);
                        }
                    }
                    else if(cx < 0 && cy > 0)                 //第三象限
                    {
                        if(Math.abs(cx) < Math.abs(cy))       //bottom
                        {
                            background_img_change_pos(this, sub_img_PTZ4);
                        }
                        else if(Math.abs(cx) > Math.abs(cy))  //left
                        {
                            background_img_change_pos(this, sub_img_PTZ5);
                        }
                    }
                    else if(cx > 0 && cy < 0)                 //第一象限
                    {
                        if(Math.abs(cx) < Math.abs(cy))       //top
                        {
                            background_img_change_pos(this, sub_img_PTZ2);
                        }
                        else if(Math.abs(cx) > Math.abs(cy))  //right
                        {
                            background_img_change_pos(this, sub_img_PTZ3);
                        }
                    }
                    else if(cx > 0 && cy > 0)                 //第四象限
                    {
                        if(ox < oy)                   //bottom
                        {
                            background_img_change_pos(this, sub_img_PTZ4);
                        }
                        else if(ox > oy)              //right
                        {
                            background_img_change_pos(this, sub_img_PTZ3);
                        }
                    }
                }
            };
