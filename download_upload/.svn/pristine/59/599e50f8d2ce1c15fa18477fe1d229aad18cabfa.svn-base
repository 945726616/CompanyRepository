    function software_init()
    {
        function create_mining_software(obj)
        {
            window.ms = new class_mining_software();
            var currheight = document.documentElement.clientHeight,
                currwidth = document.documentElement.clientWidth;
            window.onresize = function() {
                if(currheight != document.documentElement.clientHeight || currwidth != document.documentElement.clientWidth)
                {
                    window.ms.ctrl({type:"update"});
                }
                currheight = document.documentElement.clientHeight;
                currwidth = document.documentElement.clientWidth;
            };
            window.onunload = function() {
                software_uninit();
            };
        };
        function on_event(obj)
        {
            switch(obj.type)
            {
                case "ready":
                    {
                        create_mining_software();
                        break;
                    }
                default:
                    log("error:on_event's obj.type is not exist in software_init.");
            }
        };
        window.acs = new class_account ({on_event:on_event});
        acs.ctrl({type:"begin"});
    }
    window.software_init = software_init;
    function software_uninit()
    {
        window.ms.ctrl({type:"destroy"});
    }
    window.software_uninit = software_uninit;

