        function ccm_add_sub_device_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&& msg.ret.code == "")
            {
                add_device(msg.info, ref.sn);
            }
            else if(msg.ret.sub)
            {
                if(msg.ret.sub=="accounts.user.offline"){
                   // system_tooltip({parent:add_device_sn, color:"red", position:"right", disappear_way:"time", str:mcs_offline+ "."});
                   //create_device_invalid_div();
                   net_setting({sn:add_device_sn.value, pwd:acs.ctrl({type:"md5_pass", data:{pwd:add_device_pw.value}})});
                }
                else if(msg.ret.sub=="accounts.pass.invalid")
                {
                   system_tooltip({parent:add_device_pw, color:"red", position:"right", disappear_way:"time", str:mcs_invalid_password + "."});   
                }
                else if(msg.ret.sub=="accounts.user.invalid")
                {  
                   system_tooltip({parent:add_device_sn, color:"red", position:"right", disappear_way:"time", str: mcs_device_not_exist+ "."});  
                     
                }
                else if(msg.ret.sub=="ccms.system.err"){
                  
                }
                else if(msg.ret.sub == "subdev.exceed.device")
                {
                    g_ref.system_prompt_box(mcs_devices_in_the_account_overrun, -10, -200);
                }
                else if(msg.ret.sub == "subdev.exceed.user")
                {
                    g_ref.system_prompt_box(mcs_exceed_device, -10, -200);  
                }
                return -1;
            }
            if(ref.pass_length<6)
            {
               system_pop_confirm_box({str:mcs_password_too_simple_hint, callback_func:
                       function(obj){
                             pos = g_ref.device_list_link.pos("sn_" + msg.Info.sn);
                             if(g_ref.device_list_obj["sn_" + g_ref.select_device_ipc.toLowerCase()].state.online == "Online")
                             {
                  	                $(".device_list_li_span:eq(" + pos + ")").click();
                             }
                             parent_callback({type:"refresh_menu",data:3});
               }});
            }
        }
