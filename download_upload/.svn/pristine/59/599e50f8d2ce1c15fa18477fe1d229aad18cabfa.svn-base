        function ifame_on_load()
        {
            var doc = iframe.contentDocument || iframe.contentWindow.document;
            if (doc && ((undefined == doc.readyState) || (doc.readyState == "complete")))
            {
                if(1 == flag)
                {
                    var msg = doc.body.innerHTML,
                        param = null,
                        s = msg, start = s.indexOf("("), end = s.lastIndexOf(")");

                    if ((end > (start + 2))
                            && ('{' == s.charAt(start + 1))
                            && ('}' == s.charAt(end - 1))) {
                        try {
                            param = meval(s.substring(start + 1, end));
                        } catch (err) {
                            param = "error";
                        }
                    }


                    if (("error" == param) || (null == param) || (null == param.data) || ("" != param.data.ret.code))
                    {
                        log("upload error");
                    }
                    else
                    {
                        callback();
                        log("upload succeed");
                        flag = 0;
                    }
                    iframe_build();
                }
            }
        }
        /* must use attach to iframe */
        me_parent.appendChild(icon);
        me_parent.appendChild(iframe);
        evt.bind(iframe, 'load', ifame_on_load);

        icon[s_style][s_cssText] = "border:none;padding:0px;margin:0px;position:absolute;top:0px;";
        iframe[s_style][s_cssText] = "border:none;padding:0px;margin:0px;width:32px;height:32px;position:absolute;" +  hack.css_alpha(alpha);
        iframe.title =  "upload file here";
        iframe.src = "";
        iframe_build();
    }
