        function get_device_info_request(obj)
        {
            var node, node_sn, pnode, pnode_sn;

            node_sn = g_ref.select_device_ipc;
            node = g_ref.device_list_obj["sn_" + node_sn];
            if(!node)
            {
              //  log("Please select the specific device in get_device_info_request.");
                return;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }
            acs.send_msg("ccm_dev_info_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn}},
                    function(type, msg, ref){get_device_info_response(type, msg, ref);}, {ip:pnode.ip, to:"ccm"});
        }
        function get_device_info_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                if(msg.img_ver < "12.10.17.23.45")
                {
                    old_version = 1;
                    get_network_interfaces_request({select:mcs_ethernet});
                }
                else
                {
                    old_version = 0;
                    ccm_get_network_info_request("all");
                    mx("#button_connect").style.display = "none";
                }
            }
            else
            {
                log("error:ccm_dev_info_get_ack return Result error in class_options_network.");
                return -1;
            }
        }
        function ccm_get_network_info_request(filter)
        {
            var node, node_sn, pnode, pnode_sn;

            node_sn = g_ref.select_device_ipc;
            node = g_ref.device_list_obj["sn_" + node_sn];
            if(!node)
            {
               // log("Please select the specific device in ccm_get_network_info_request.");
                return;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }
            set_wifi_config_response_ok = 0;
            acs.send_msg("ccm_net_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn},
                tokens:["eth0", "ra0"], items:[filter, filter], force_scan:filter=="all"?0:1},
                function(type, msg, ref){
                    if(filter == "all")
                        ccm_get_network_info_response(type, msg, ref);
                    else
                        ccm_get_wifi_client_info_response(type, msg, ref);}, {ip:pnode.ip, to:"ccm"});
        }
        function ccm_get_network_info_response(type, msg, ref)
        {
            if(!me_parent) return;
            g_ref.system_stop_wait();
            clearInterval(refresh_timer);
            if(msg && msg.ret&&msg.ret.code == "")
            {
                var info = msg.info;
                if(info.ifs)
                {
                    var i, length = info.ifs.length, inner_html = "";

                    for(i = 0; i < length; ++i)
                    {
                        if(info.ifs[i].token == "eth0")
                            if(ref.select == mcs_ethernet)
                                inner_html += "<option value ='" + i + "' selected='selected'>" + mcs_ethernet + "</option>";
                            else
                                inner_html += "<option value ='" + i + "'>" + mcs_ethernet + "</option>";
                        else if(info.ifs[i].token == "ra0")
                            if(ref.select == mcs_wifi)
                                inner_html += "<option value ='" + i + "' selected='selected'>" +mcs_wifi + "</option>";
                            else
                                inner_html += "<option value ='" + i + "'>" + mcs_wifi + "</option>";
                    }
                    $(select_nic).html(inner_html);
                    select_nic.change_value = function(obj) {
                        if(select_net==mcs_wifi)
                        {
                            select_nic[1].selected=true;
                            select_net="";
                        }
                        else if (select_net==mcs_connnected)
                        {
                            select_nic[0].selected=true;
                            select_net="";
                        }
                        $(select_nic).tzSelect();
                        nic_enabled_status_flag = 0;
                        nic_conn_status_flag = 0;
                        ip_is_DHCP = 0;
                        if(this[this.selectedIndex].text == mcs_ethernet)
                        {
                            $("#button_setup").unbind();
                            generate_eth_setup_ex(info.ifs[this.selectedIndex]);
                        }
                        else if(this[this.selectedIndex].text == mcs_wifi)
                        {
                            $("#button_setup").unbind();
                            generate_wireless_setup_ex(info.ifs[this.selectedIndex]);
                        }
                        if(info.dns && info.dns.conf && info.dns.info)
                        {
                            if(info.dns.info.status == "ok")
                            {
                                background_img_set(mx("#dns_status"), sub_img_status0);
                                mx("#dns_status").setAttribute("title", mcs_connnected);
                            }
                            else if(info.dns.info.status == "err")
                            {
                                background_img_set(mx("#dns_status"), sub_img_status1);
                            }
                            if(info.dns.conf.mode == "dhcp")
                            {
                                input_auto_dns.value = info.dns.info.dns[0] || "0.0.0.0";
                                if(info.dns.info.dns.length > 1)
                                    input_auto_alternate_dns.value = info.dns.info.dns[1] || "0.0.0.0";
                                dns_is_DHCP = 1;
                                trigger_click(radio_auto_obtain_dns);
                            }
                            else if(info.dns.conf.mode == "static")
                            {
                                input_following_dns.value = info.dns.conf.static_dns[0] || "0.0.0.0";
                                if(info.dns.conf.static_dns.length > 1)
                                    input_following_alternate_dns.value = info.dns.conf.static_dns[1] || "0.0.0.0";
                                dns_is_DHCP = 0;
                                trigger_click(radio_use_following_dns);
                            }
                        }
                        else
                        {
                            log("info:ccm_dev_info_get_ack return info.dns || dns.conf || dns.info is null in class_options_network.");
                        }
                        $("#button_setup").bind("click", function() {
                            var ip_address, ip_refresh = "", origin_connected = "";

                            if(g_ref.select_device_ipc && !g_ref.device_list_obj["sn_" + g_ref.select_device_ipc].parent_sn)          //Direct Connect
                            {
                                if(origin_ethernet_addr == window.location.host)
                                {
                                    origin_connected = origin_ethernet_addr;
                                    if(select_nic[select_nic.selectedIndex].text == mcs_ethernet && radio_auto_obtain_ip.checked && !ip_is_DHCP)
                                    {
                                        ip_refresh = origin_connected;
                                    }
                                }
                                else if(origin_wireless_addr == window.location.host)
                                {
                                    origin_connected = origin_wireless_addr;
                                    if(select_nic[select_nic.selectedIndex].text == mcs_wifi && radio_auto_obtain_ip.checked && !ip_is_DHCP)
                                    {
                                        ip_refresh = origin_connected;
                                    }
                                }
                                if(radio_use_following_ip.checked)
                                {
                                    ip_address = input_following_ip_address.value;
                                    if(select_nic[select_nic.selectedIndex].text == mcs_ethernet)
                                    {
                                        if((ip_address != origin_ethernet_addr) && (origin_connected == origin_ethernet_addr))
                                            ip_refresh = ip_address;
                                    }
                                    else if(select_nic[select_nic.selectedIndex].text == mcs_wifi)
                                    {
                                        if((ip_address != origin_wireless_addr) && (origin_connected == origin_wireless_addr))
                                            ip_refresh = ip_address;
                                    }
                                }
                            }
                            else
                            {
                                if(radio_auto_obtain_ip.checked)
                                {
                                    if(!ip_is_DHCP)
                                    {
                                        if(select_nic[select_nic.selectedIndex].text == mcs_wifi)
                                        {
                                            ip_refresh = origin_wireless_addr;
                                        }
                                        else if(select_nic[select_nic.selectedIndex].text == mcs_ethernet)
                                        {
                                            ip_refresh = origin_ethernet_addr;
                                        }
                                    }
                                }
                                else if(radio_use_following_ip.checked)
                                {
                                    ip_address = input_following_ip_address.value;
                                    if(select_nic[select_nic.selectedIndex].text == mcs_wifi)
                                    {
                                        if(ip_address != origin_wireless_addr)
                                            ip_refresh = ip_address;
                                    }
                                    else if(select_nic[select_nic.selectedIndex].text == mcs_ethernet)
                                    {
                                        if(ip_address != origin_ethernet_addr)
                                            ip_refresh = ip_address;
                                    }
                                }
                            }
                            if(ip_refresh)
                            {
                                var type = g_ref.device_list_obj["sn_" + g_ref.select_device_ipc].type;
                                if((type == "IPC"||type == "BOX") && g_ref.device_list_obj["sn_" + g_ref.select_device_ipc].isParent)                //Direct Connect
                                {
                                    system_pop_confirm_box({str:mcs_modify_network_prompt, callback_func:function(){
                                        ccm_set_network_info_request({type:select_nic[select_nic.selectedIndex].text,
                                            ori_net_info:info, to:ip_refresh + ":" + location.port, select:select_nic[select_nic.selectedIndex].text});
                                    }});
                                }
                                else                                                                                                //Through the server
                                {
                                    system_pop_confirm_box({str:mcs_modify_network_prompt, callback_func:function(){
                                        ccm_set_network_info_request({type:select_nic[select_nic.selectedIndex].text,
                                            ori_net_info:info, to:ref.ip, select:select_nic[select_nic.selectedIndex].text});
                                    }});
                                }
                            }
                            else
                            {
                                system_pop_confirm_box({str:mcs_modify_network_prompt, callback_func:function(){
                                    ccm_set_network_info_request({type:select_nic[select_nic.selectedIndex].text,
                                        ori_net_info:info, to:ip_refresh, select:select_nic[select_nic.selectedIndex].text});
                                }});
                            }
                        });
                    };
                    
                    select_nic.change_value();
                }
                else
                {
                    log("error:ccm_net_get_ack return ifaces is null in class_options_network.");
                    return -1;
                }
            }
            else
            {
                log("eror:ccm_net_get_ack return Result error in class_options_network.");
            }
        }
