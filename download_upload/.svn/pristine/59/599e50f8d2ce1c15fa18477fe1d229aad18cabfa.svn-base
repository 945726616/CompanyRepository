        function ccm_devs_get_ack(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret && msg.ret.code == "")
            {
                var i, length, name, devices;
                if(ref.Refresh)
                {
                    g_ref.device_list_link.empty();  
                    g_ref.device_list_obj={};  
                }
                if(msg.total == 1)
                {
                    g_ref.view_split_count = g_ref.playback_split_count = 1;
                }
                name = ref.nick || ref.sn;
                g_ref.device_list_obj["sn_" + ref.sn] = {name:name, type:"MS", ip:ref.ip, isParent:true,
                    state:{online:"Online", view:0, recording:0, call:0, playback:0}};
                devices = msg.devs;
                if(devices)
                {
                    length = devices.length;
                    for(i = 0; i < length ; ++i)
                    {
                        name = devices[i].nick || devices[i].sn;
                        Version="";

                        if(devices[i].stat === "Online")
                            g_ref.device_list_link.add("sn_" + devices[i].sn);
                        else if(devices[i].stat === "InvalidAuth")
                            g_ref.device_list_link.add("sn_" + devices[i].sn);
                        else if(devices[i].stat === "Offline")
                            g_ref.device_list_link.add("sn_" + devices[i].sn);

                        if(devices[i].img_ver!="")
                        {
                           if(devices[i].img_ver>= "14.01.23.12.30")
                           {
                                   Version=version_Category.wifi_guide_version;
                           }
                           else if(devices[i].img_ver >= "13.09.14.12.30")
                           {
                                  Version =version_Category.guest_version;
                           }
                           else if(devices[i].img_ver >= "13.05.20.17.57")
                           {
                                 Version=version_Category.Schedule_recording;
                           }
                           else  if(devices[i].img_ver >= "13.05.09.17.57")
                           {
                                  Version=version_Category.Version_recording;
                           }
                           else if(devices[i].img_ver>"13.04.01.01.35")
                           {
                              	  Version=version_Category.Info_version
                           }else if(devices[i].img_ver <= "13.04.01.01.35" && msg.ver > "13.01.01.01.35")
                           {
                           		  Version=version_Category.Snapshot_version;
                           }else
                           {
                                Version=version_Category.Basic_version;   
                           }
                        }
                        else
                        {
                             Version=version_Category.Basic_version; 
                        }
                        var view,recording,call,playback,isDeleteFlag;
                        if(devices[i].sn==g_ref.select_device_ipc){
                             view=ref.select_device_ipc_view;
                             call=ref.select_device_ipc_call;
                             recording=ref.select_device_ipc_recording;
                             playback=ref.select_device_ipc_playback;
                             isDeleteFlag=1;
                        }
                        else {
                            view=0;
                            recording=0;
                            call=0;
                            playback=0;
                        }
                        g_ref.device_list_obj["sn_" + devices[i].sn] = {name:name,Version:Version,sys_version:devices[i].img_ver,param:devices[i].p,type:devices[i].type, ip:devices[i].addr, isParent:false, parent_sn:ref.sn,
                            state:{online:devices[i].stat, view:view, recording:recording, call:call, playback:playback}};
                        //if(!isDeleteFlag)
                        //    parent_callback({type:"delete_ipc", data:{node_sn:g_ref.select_device_ipc}});
                    }
                  
                }
                else
                {
                   //log("info:ccm_devs_get_ack return Devices is null in class_mining_software.");
                }
                if(!ref.Refresh)
                {
                     main_page_obj.ctrl({type:"create", data:{type:ref.type, sn:ref.sn}});
                }else
                {
                     main_page_obj.ctrl({type:"refresh_device_list",data:3,sn:ref.sn});    
                }
                if(re_login)
                {
                     re_login=false;
                     acs.ctrl({type:"Duplicate_send_msg"});
                }
                
            }
            else
            {
                log("error:ccm_devs_get_ack return Result error in class_mining_software.");
                return -1;
            }
        }
