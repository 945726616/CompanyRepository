        function ccm_get_osds_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                if(msg.osd.text_enable)
                {
                    $("#checkbox_display_name").iButton("toggle", true);
                }
                else
                {
                	$("#checkbox_display_name").iButton("toggle", false);
                }
                input_display_name.value = msg.osd.text;
                if(msg.osd.date.date_enable)
                {
                    $("#checkbox_display_date").iButton("toggle", true);
                    
                    var i, length;

                    length = select_date.length;
                    for(i = 0; i < length; ++i)
                    {
                        if(select_date[i].text == msg.osd.date.format)
                        {
                            select_date[i].selected = true;
                        }
                    }
                    
                }
                else
                {
                    select_date.disabled = true;
                    $("#checkbox_display_date").iButton("toggle", false);
                }
                if(msg.osd.date.time_enable)
                {

                    $("#checkbox_display_time").iButton("toggle", true);
                    if(msg.osd.date.enable_12h)
                        select_hour[0].selected = true;
                    else
                        select_hour[1].selected = true;
                }
                else
                {
                    select_hour.disabled = true;
                    $("#checkbox_display_time").iButton("toggle", false);
                }
                if(msg.osd.week)
                {

                    $("#checkbox_display_weeks").iButton("toggle", true);
                }else
                {
                	  $("#checkbox_display_weeks").iButton("toggle", false);
                }
            }
            else
            {
                log("error:ccm_osd_get_ack return Result error in class_options_OSD.");
                return -1;
            }
            $(select_date).tzSelect();
            $(select_hour).tzSelect();
           
        }
