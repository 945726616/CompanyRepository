            return 0;
        }
        function get_device_info_response(type, msg, ref)
        {
            var lang_choice =local_storage.get("language_choice_info");
            var nav_lang = navigator.language || navigator.browserLanguage || navigator.userLanguage || navigator.systemLanguage || "",
            nav_lang=nav_lang.toLowerCase();
            if(!lang_choice)
            {
                lang_choice=nav_lang;
				
                if(lang_choice.indexOf("zh")>=0)
                {
                   lang_choice="zh";    
                }
            }
            var mfc=true,get_ipc=true;
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                input_manufacturer.value         = msg.mfc;
                input_model.value                = msg.model;
                input_firmware_version.value     = msg.img_ver;
                input_device_serial_number.value = msg.sn;
                if(msg.p)
                {
                    for(i=0;i<msg.p.length;++i)
                    {
                        if(msg.p[i].n=="s.sensor" && msg.p[i].v=="fail")
                        {
                            $(device_status_content).show();   
                        }
                        if(msg.p[i].n=="s.mfc_"+lang_choice)
                        {
                             input_mfc.value =msg.p[i].v;
                             mfc=false;
                             $(mfc_content).show();
                        }
                        else if(msg.p[i].n=="s.mfc" && mfc)
                        {
                             input_mfc.value =msg.p[i].v;
                             $(mfc_content).show();       
                        }
                        if(msg.p[i].n=="s.model")
                        {
                              input_model.value=msg.p[i].v; 
                               
                        }
                        if(msg.p[i].n=="s.logo")
                        {
                            get_ipc=false;
                            $(logo_content).show();
                            var Images =new Image();
                            Images.src=msg.p[i].v;
                            Images.width="80";
                            Images.height="80";
                            $("#log_id").append(Images);
                        }
                    }
                }
                else
                {
                   $(device_status_content).hide();   
                }
                if(get_ipc && msg.img_ver && msg.img_ver < "13.09.01.00.00")
                {
                    acs.send_msg_ex(g_ref.select_device_ipc, "ccms_get_ipc_config_req", {}, function(type, msg, ref){ccms_get_ipc_config_ack(type, msg, ref);});

                }
            }
            else
            {
                log("error:ccm_dev_info_get_ack return Result error in class_options_product_info.");
                return -1;
            }
        }
        function ccms_get_ipc_config_ack(type,msg,ref)
        {
            if(msg && msg.ret&&msg.ret.code == "")
            {
               if(msg.system)
               {
                  var msgsystem=msg.system;
                  msgsystem= msgsystem.replace(/s.oid:/,"s_oid:");
                  msgsystem= msgsystem.replace(/s.mfc:/,"s_mfc:");
                  msgsystem= msgsystem.replace(/s.model:/,"s_model:");
                  msgsystem= msgsystem.replace(/s.logo:/,"s_log:");
                
                  var systems=eval("("+msgsystem+")"); 
                  if(systems.oinfo)
                  {
                    if(systems.oinfo.s_mfc)
                    {
                        $(mfc_content).show();
                        input_mfc.value = systems.oinfo.s_mfc;   
                    }
                    if(systems.oinfo.s_model)
                    {
                        input_model.value=systems.oinfo.s_model;
                    }
                    if(systems.oinfo.s_log)
                    {
                        $(logo_content).show();
                        var Images =new Image();
                        Images.src=systems.oinfo.s_log;
                        Images.width="80";
                        Images.height="80";
                        mx("#log_id")[s_innerHTML]="";
                        $("#log_id").append(Images);
                    }   
                  }
               }
            }
        }
        function ccm_oinfo_get_ack(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                if(msg.info.model)
                {
                   input_model.value =msg.info.model;
                }
                if(msg.info.mfc)
                {
                   input_mfc.value=msg.info.mfc;
                   $(mfc_content).show();
                }
                if(msg.info.logo)
                {
                   $(logo_content).show();
                   var Images =new Image();
                   Images.src=msg.info.logo;
                   Images.width="80";
                   Images.height="80";
                   $("#log_id").append(Images);
               }
            }
        }
