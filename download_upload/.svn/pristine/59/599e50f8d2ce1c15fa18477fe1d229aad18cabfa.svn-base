            if(client_w < g_ref.client_min_all_w)
            {
                client_w = g_ref.client_min_all_w;
            }
            if(client_h < g_ref.client_min_all_h)
            {
                client_h = g_ref.client_min_all_h;
            }
            if(client_w < g_ref.client_division_w)
            {
                g_ref.panel_box_all_w = Math.floor((client_w - g_ref.client_min_w - 20) / 2);
                g_ref.panel_box_offset = -(g_ref.panel_box_max_w - g_ref.panel_box_all_w);
            }
            else
            {
                g_ref.panel_box_all_w = g_ref.panel_box_max_w;
                g_ref.panel_box_offset = 3;
            }
            temp_w = client_w - g_ref.panel_box_all_w * 2 - 20;                                             //2 * 10 (left and right border);
            temp_h = client_h - g_ref.menu_box_h - g_ref.control_box_h - g_ref.status_bar_h - 12;           //12(margin-top)
            temp_p = temp_w / temp_h;
            if(temp_p < 16/9)
            {
                temp_h = temp_w * 9 / 16;
            }
            else
            {
                temp_w = temp_h * 16 / 9;
            }
            g_ref.main_window_w = Math.floor(temp_w);
            g_ref.main_window_h = g_ref.panel_box_h = Math.floor(temp_h);
            g_ref.panel_box_all_h = g_ref.main_window_h + 12;                                               //12(margin-top)
