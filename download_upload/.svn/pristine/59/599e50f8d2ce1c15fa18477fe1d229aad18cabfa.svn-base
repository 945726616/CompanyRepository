    Date.prototype.format = function(format)
    {
        var o = {
            "M+" : this.getMonth()+1, //month
            "d+" : this.getDate(),    //day
            "h+" : this.getHours(),   //hour
            "m+" : this.getMinutes(), //minute
            "s+" : this.getSeconds(), //second
            "q+" : Math.floor((this.getMonth()+3)/3),  //quarter
            "S" : this.getMilliseconds() //millisecond
        }
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,
                (this.getFullYear()+"").substr(4 - RegExp.$1.length));
        for(var k in o)if(new RegExp("("+ k +")").test(format))
            format = format.replace(RegExp.$1,
                    RegExp.$1.length==1 ? o[k] :
                    ("00"+ o[k]).substr((""+ o[k]).length));
        return format;
    }

    var log_dev, log_status, log_span, log_list = createLinkList(), log_flag;
    function log(info, level)
    {
        var status_content = mx("#status_content");
        if(null == log_dev)
        {
            //if(0 <= (location.search || location.hash || "").indexOf("debug=1"))
            if(status_content)
            {
                log_dev = dom_create_child(status_content, "div");
                log_status = dom_create_child(log_dev, "div", "log_status");
                log_span = dom_create_child(log_dev, "div");

                log_dev.style.cssText  = "color:#c92641;font-weight:bold";
                log_span.style.cssText = "padding-left:10px";
            }
        }
        if(info)
        {
            if(mx("#status_bar"))
            {
                        log_flag = $("#status_bar").queue("log_list").length == 0 ? 1 : 0;
                            $("#status_bar").queue("log_list", function(next){
                                $(this).animate({height:0, marginTop:"8"}, {duration:500, queue:false, complete:function(){
                                    next();
                                }})
                            })
                            .queue("log_list", function(next){
                                status_content.style.cssText =
                                    "background-image:-moz-linear-gradient(center top , #141819 0px, #37383A 100%);"
                                    +"background:-webkit-gradient(linear, 0 0, 0 bottom, from(#141819), to(#37383A));"
                                    +"background:-o-linear-gradient(top, #141819, #37383A);"
                                    +"filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#141819', endColorstr='#37383A');*background:url(about:blank);";
                                    log_span.innerHTML = info;
                                    next();
                            })
                            .queue("log_list", function(next){
                                $(this).animate({height:g_ref.status_bar_h, marginTop:"0"}, {duration:500, queue:false, complete:function(){
                                    next();
                                }});
                            })
                            .delay(4500, "log_list")
                            .queue("log_list", function(next){
                                $(this).animate({height:0, marginTop:"8"}, {duration:500, queue:false, complete:function(){
                                    if($("#status_bar").queue("log_list").length == 0)
                                    {
                                        $("#status_bar").queue("log_list", function(next){
                                            status_content.style.cssText = "";
                                            log_span.innerHTML = "";
                                            $(this).animate({height:g_ref.status_bar_h, marginTop:"0"}, {duration:500, queue:false, complete:function(){
                                                next();
                                            }});
                                        });
                                    }
                                    next();
                                }});
                            });
                    if(log_flag)
                        $("#status_bar").dequeue("log_list");
            }

            var date  = new Date().format("yyyy-MM-dd hh:mm:ss");
            if(log_list.length == 10)
            {
                log_list.del(log_list.get_first().data);
            }
            log_list.add({date:date, info:info});
        }
        else
        {
            return log_list;
        }
        return 0;
    }
    function get_version_Category()
    {
        var device_version;
        
        if(g_ref.device_list_obj["sn_" + g_ref.select_device_ipc.toLowerCase()])   
           device_version=g_ref.device_list_obj["sn_" + g_ref.select_device_ipc.toLowerCase()].Version;
        else if(g_ref.device_list_obj["sn_" + g_ref.select_device_ipc.toUpperCase()])
           device_version=g_ref.device_list_obj["sn_" + g_ref.select_device_ipc.toUpperCase()].Version;
        
        if(device_version==version_Category.Basic_version)
        {
              return version_Category.Basic_version;
        }
        else if(device_version==version_Category.Snapshot_version)
        {
              return version_Category.Snapshot_version;
        }
        else if(device_version==version_Category.Info_version)
        {
              return version_Category.Info_version;
        }
        else if(device_version==version_Category.Version_recording)
        {
              return version_Category.Version_recording;
        }else if(device_version==version_Category.Schedule_recording)
        {
              return version_Category.Schedule_recording;
        }else if(device_version==version_Category.guest_version)
        {
              return  version_Category.guest_version; 
        }else if(device_version==version_Category.wifi_guide_version)
        {
              return  version_Category.wifi_guide_version;
        }
            
    }
    window.get_version_Category=get_version_Category;
    function determine_the_version(obj)
    {
        if(obj.type=="Positive")
        {
           if(get_version_Category()>=obj.version)
           {
               return true;
           }else
           {
              return false;    
           }
        }else if(obj.type=="Anti")
        {
           if(get_version_Category()<=obj.version)
           {
               return true;
           }else
           {
              return false;    
           }    
        }
        else
        {
            if(get_version_Category()==obj.version)
            {
                return true;
            }else
            {
               return false;    
            }
                
        }
            
    }
    window.determine_the_version=determine_the_version;
