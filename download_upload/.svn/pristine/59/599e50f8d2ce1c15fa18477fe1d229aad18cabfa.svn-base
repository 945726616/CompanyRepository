                var e = evt || window.event, target = e.target || e.srcElement;
                if(!target) return;
                delete_divs = mx(".device_delete_div");
                delete_imgs = mx(".device_delete_img");
                length = delete_divs.length;
                for(i = 0; i < length; ++i)
                {
                    if(delete_divs[i] === target || delete_imgs[i] === target) return;
                }
                --length;
                for(; length >= 0; --length)
                {
                    list_classDom[length].firstChild.style.width = "152px";
                    delete_divs[length].onclick = null;
                    delete_divs[length].parentNode[s_removeChild](delete_divs[length]);
                }
                j_page.unbind("mouseup", temp_func);
            };
            j_page.bind("mouseup", temp_func);
            return 0;
        }
        function add_device(info, psn)
        {
            var name,Version;
            
            name = info.nick || info.sn;
            if(info.stat === "Online")
                g_ref.device_list_link.add("sn_" + info.sn);
            else if(info.stat === "InvalidAuth")
                g_ref.device_list_link.add("sn_" + info.sn);
            else if(info.stat === "Offline")
                g_ref.device_list_link.add("sn_" + info.sn);
           
            if(g_ref.device_list_link.length == 1)
                g_ref.select_device_ipc = info.sn;
            if(device_add_menu_dom)
            {
                page[s_removeChild](device_add_menu_dom);
                device_add_menu_dom = null;
            }
            if(info.img_ver!="")
            {
                if(info.img_ver>= "14.01.23.12.30")
                {
                       Version=version_Category.wifi_guide_version;
                }
                else if(info.img_ver >= "13.09.14.12.30")
                {
                     Version =version_Category.guest_version;
                }
                else if(info.img_ver>= "13.05.20.17.57")
                {
                     Version=version_Category.Schedule_recording;
                }
                else if(info.img_ver >= "13.05.09.17.57")
                {
                      Version=version_Category.Version_recording;
                }
                else if(info.img_ver>"13.04.01.01.35")
                {
                      Version=version_Category.Info_version
                }
                else if(info.img_ver <= "13.04.01.01.35" && info.img_ver> "13.01.01.01.35")
                {
                      Version=version_Category.Snapshot_version;
                }else
                {
                      Version=version_Category.Basic_version;   
                }
            }
            else
            {
                Version=version_Category.Basic_version; 
            }
            g_ref.device_list_obj["sn_" + info.sn] = {name:name,Version:Version,param:info.p, type:info.type, ip:info.addr, isParent:false, parent_sn:psn, state:{online:info.stat, play:"stop"}};
            create_device_list();
           
        }
        
        
        function delete_device(sn)
        {
            var pos = g_ref.device_list_link.pos("sn_" + sn),
                list_classDom = mx(".device_list_li"),
                j_list_classDom, now_sn, now_pos;

            parent_callback({type:"delete_ipc", data:{node_sn:sn}});
            j_list_classDom = $(list_classDom[pos]);
            //Animation
            if(!j_list_classDom.is(":animated")) {
                j_list_classDom.animate({width:"0px"}, {queue:true, duration:200}).slideUp(200,
                        function(){
                            //Dom destroy
                            list_classDom[pos][s_innerHTML] = "";
                            list_classDom[pos].parentNode[s_removeChild](list_classDom[pos]);
                            list_classDom[pos] = null;
                            //Data adjustment
                            g_ref.device_list_link.del("sn_" + sn);
                            delete g_ref.device_list_obj["sn_" + sn];
                            
                        });
            }
            if(g_ref.device_list_link.length)
            {
                now_sn = g_ref.device_list_link.get_first().data.substr(3);
                now_pos = g_ref.device_list_link.pos("sn_" + now_sn);
                g_ref.select_device_ipc = now_sn;
                if(list_span_classDom[now_pos])
                    $(".device_list_li_span:eq(" + now_pos + ")").click();
            }
            else
                g_ref.select_device_ipc = "";
            return 0;
        }
