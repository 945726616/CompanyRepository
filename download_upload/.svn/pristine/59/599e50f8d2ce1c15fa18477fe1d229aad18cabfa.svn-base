        function ccm_set_network_info_request(obj)
        {
            var node, node_sn, pnode, pnode_sn,
                i, length, count = 0, net_info = {}, now_ifs, now_net_info = {};

            node_sn = g_ref.select_device_ipc;
            node = g_ref.device_list_obj["sn_" + node_sn];
            if(!node)
            {
                //log("Please select the specific device in ccm_set_network_info_request.");
                return;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }
            obj_merge(net_info, obj.ori_net_info);
            for(i = 0; i < net_info.ifs.length; ++i)
            {
                if(net_info.ifs[i].enabled)
                    ++ count;
            }
            now_ifs = net_info.ifs[select_nic.selectedIndex];
            if(obj.type == mcs_ethernet)
            {
                if(nic_switch_checkbox.checked)
                {
                    if(nic_enabled_status_flag)
                    {
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:1};
                        if(radio_auto_obtain_ip.checked)
                        {
                            now_net_info.ifs["ipv4"] = {conf:{enabled:now_ifs.ipv4.conf.enabled, mode:"dhcp", static_ip:"", debug_ip:""}};
                        }
                        else if(radio_use_following_ip.checked)
                        {
                            now_net_info.ifs["ipv4"] = {conf:{enabled:now_ifs.ipv4.conf.enabled, mode:"static",
                                static_ip:{addr:input_following_ip_address.value, gw:input_following_gateway.value,
                                    mask:now_ifs.ipv4.conf.static_ip ? now_ifs.ipv4.conf.static_ip.mask : ""},
                                debug_ip:now_ifs.ipv4.conf.debug_ip}};
                            if(subnet_mask_available_and_trans({data:input_following_subnet_mask.value}) >= 0)
                                now_net_info.ifs.ipv4.conf.static_ip.mask = input_following_subnet_mask.value;
                            else
                            {
                                system_pop_confirm_box({alert:true, str:mcs_subnet_mask_wrong_format});
                                return;
                            }
                        }
                    }
                    else
                    {
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:1};
                    }
                }
                else
                {
                    if(count < 1)
                    {
                        system_pop_confirm_box({alert:true, str:mcs_not_allowed_close_two_network_cards});
                        return;
                    }
                    else
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:0};
                }
            }
            else if(obj.type == mcs_wifi)
            {
                if(nic_switch_checkbox.checked)
                {
                    if(nic_enabled_status_flag)
                    {
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:1};
                        if(mx("#nic_mode_switch_checkbox").checked)             //client
                        {
                            now_net_info.ifs["phy"] = {conf:{mode:"wificlient", mtu:now_ifs.phy.conf.mtu}};
                            if(radio_auto_obtain_ip.checked)
                            {
                                now_net_info.ifs["ipv4"] = {conf:{enabled:now_ifs.ipv4.conf.enabled, mode:"dhcp", static_ip:"", debug_ip:""}};
                            }
                            else if(radio_use_following_ip.checked)
                            {
                                now_net_info.ifs["ipv4"] = {conf:{enabled:now_ifs.ipv4.conf.enabled, mode:"static",
                                    static_ip:{addr:input_following_ip_address.value, gw:input_following_gateway.value,
                                        mask:now_ifs.ipv4.conf.static_ip ? now_ifs.ipv4.conf.static_ip.mask : ""},
                                    debug_ip:now_ifs.ipv4.conf.debug_ip}};
                                if(subnet_mask_available_and_trans({data:input_following_subnet_mask.value}) >= 0)
                                    now_net_info.ifs.ipv4.conf.static_ip.mask = input_following_subnet_mask.value;
                                else
                                {
                                    system_pop_confirm_box({alert:true, str:mcs_subnet_mask_wrong_format});
                                    return;
                                }
                            }
                            now_net_info.ifs["wifi_client"] = {conf:{enabled:1, ssid:mx("#select_network_edit").value,
							    usr:mx("#select_network_edit").value,
                                key:select_network_password.value}};
                        }
                        else                                                    //adhoc
                        {
                            now_net_info.ifs["phy"] = {conf:{mode:"adhoc", mtu:now_ifs.phy.conf.mtu}};
                            now_net_info.ifs["dhcp_srv"] = {conf:{enabled:now_ifs.dhcp_srv.conf.enabled, gw:input_ap_gateway.value,
                                ip_start:input_ap_start_address.value, ip_end:input_ap_end_address.value}}
                        }
                    }
                    else
                    {
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:1};
                    }
                }
                else
                {
                    if(count < 1)
                    {
                        system_pop_confirm_box({alert:true, str:mcs_not_allowed_close_two_network_cards});
                        return;
                    }
                    else
                        now_net_info["ifs"] = {token:now_ifs.token, enabled:0};
                }
            }
            if(nic_enabled_status_flag)
            {
                if(radio_auto_obtain_dns.checked)
                {
                    now_net_info["dns"] = {conf:{enalbed:net_info.dns.conf.enabled, mode:"dhcp", static_dns:net_info.dns.conf.static_dns}};
                }
                else if(radio_use_following_dns.checked)
                {
                    now_net_info["dns"] = {conf:{enalbed:net_info.dns.conf.enabled, mode:"static",
                        static_dns:[input_following_dns.value, input_following_alternate_dns.value]}};
                }
            }
            acs.send_msg("ccm_net_set", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn}, info:now_net_info},
                function(type, msg, ref){ccm_set_network_info_response(type, msg, ref);}, {ip:pnode.ip, to:"ccm", sn:node_sn, ip_refresh:obj.to, select:obj.select});
        }
        function ccm_set_network_info_response(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&&msg.ret.code == "")
            {
                g_ref.system_prompt_box(mcs_set_successfully, 240, -200);
                //g_ref.system_wait_div(mcs_reconnect_please_wait + "...");
                if(ref.ip_refresh && ref.ip_refresh != ref.ip)
                {
                    refresh_timer = setInterval(function() {
                        acs.send_msg("cacs_check_nid_req", {nid:acs.create_nid(acs.ctrl({type:"get_info"}))},
                            function(type, msg, ref) {on_cacs_check_nid_ack(type, msg, ref);}, {sn:ref.sn, ip:ref.ip_refresh, to:"ccm"});
                    }, 1500);
                }
                else
                {
                    refresh_timer = setInterval(function() {
                        acs.send_msg("ccm_net_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:ref.sn}},
                            function(type, msg, ref){ccm_get_network_info_response(type, msg, ref);}, {ip:ref.ip, to:"ccm", select:ref.select});
                    }, 3500);
                }
            }
            else
            {
                log("eror:ccm_net_set_ack return Result error in class_options_network.");
            }
        }
