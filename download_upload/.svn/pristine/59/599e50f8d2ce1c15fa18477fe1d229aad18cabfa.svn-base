        function get_current_date(obj)
        {
            var i, length = 0, begin_event, end_event, pos,
                offset_millisecond, offset_date,
                between_millisecond = (search_end_date - search_begin_date);

            if(undefined !== obj.date)
            {
                offset_date = obj.date;
                if(event_array)
                    length = event_array.length;
                for(i = 0; i < length; ++i)
                {
                    if(offset_date < event_array[i].date)
                    {
                        if(event_array[i].type == "end")
                        {
                            return {state:"normal", offset_date:offset_date,
                                offset_value:Math.round(playback_slider_max_range * (offset_date - search_begin_date) / between_millisecond)};
                        }
                        else if(event_array[i].type == "begin")
                        {
                            offset_date = event_array[i].date;
                            return {state:"jump", offset_date:offset_date,
                                offset_value:Math.round(playback_slider_max_range * (offset_date - search_begin_date) / between_millisecond)};
                        }
                    }
                }
                //Not in any event in front of That is the last event behind the event
                if(event_array[length - 1].type == "begin")
                {
                    return {offset_date:offset_date,
                        offset_value:Math.round(playback_slider_max_range * (offset_date - search_begin_date) / between_millisecond)};
                }
                else
                {
                    return {offset_date:0, offset_value:0};
                }
            }
            else if(undefined !== obj.value)
            {
                pos = obj.value / playback_slider_max_range;
                offset_millisecond = Math.round(between_millisecond * pos);
                offset_date = search_begin_date + offset_millisecond;
                if(event_array && event_array.length)
                    length = event_array.length;
                else return {offset_date:0, offset_value:0};
                for(i = 0; i < length; ++i)
                {
                    if(offset_date < event_array[i].date)
                    {
                        if(event_array[i].type == "end")
                        {
                            return {state:"normal", offset_date:offset_date, offset_value:obj.value};
                        }
                        else if(event_array[i].type == "begin")
                        {
                            offset_date = event_array[i].date;
                            return {state:"jump", offset_date:offset_date,
                                offset_value:Math.round(playback_slider_max_range * (offset_date - search_begin_date) / between_millisecond)};
                        }
                    }
                }
                //Not in any event in front of That is the last event behind the event
                if(event_array[length - 1].type == "begin")
                {
                    return {offset_date:offset_date, offset_value:obj.value};
                }
                else
                {
                    return {offset_date:0, offset_value:0};
                }
            }
        }
        function play_playback_handle(obj)
        {
            var between_milliseconds = (search_end_date - search_begin_date);
            var exchange_rate = playback_slider_max_range / (between_milliseconds / 1000),
                current_value,current_time, next_time;

            current_value = parseInt(mx(".fd-slider-handle", mx("#fd-slider-playback_slider"))[0].getAttribute("aria-valuenow"), 10);
            passed_second = 1;
            starting_line = current_value;
            if(current_value < playback_slider_max_range)
            {
                current_time = get_current_date({value:current_value});
                view_page_obj.ctrl({type:"seek_ipc", data:{type:"time", date:current_time.offset_date, inner_window_info:obj.inner_window_info}});
                next_time = get_current_date({value:current_time.offset_value + exchange_rate});
                fdSliderController.increment("playback_slider", next_time.offset_value - current_time.offset_value);
                if(play_pause_timer)
                    clearInterval(play_pause_timer);
                play_pause_timer = setInterval(
                    function() {
                        ++passed_second;
                        current_value = parseInt(mx(".fd-slider-handle", mx("#fd-slider-playback_slider"))[0].getAttribute("aria-valuenow"), 10);
                        if(current_value < playback_slider_max_range)
                        {
                            next_time = get_current_date({value:starting_line + passed_second * exchange_rate});
                            if(next_time.state == "jump")
                            {
                                passed_second = 0;
                                starting_line = next_time.offset_value;
                                view_page_obj.ctrl({type:"seek_ipc", data:{type:"time", date:next_time.offset_date, inner_window_info:obj.inner_window_info}});
                            }
                            if(!next_time.offset_date)
                            {
                                passed_second = 0;
                                clearInterval(play_pause_timer);
                                playback_control_box_obj.ctrl({type:"adjust_play_button_state", data:{playback_state:"pause"}});
                                view_page_obj.ctrl({type:"pause_ipc", data:{inner_window_info:obj.inner_window_info}});
                                fdSliderController.increment("playback_slider", playback_slider_max_range);
                                return;
                            }
                            if(Math.round(next_time.offset_value) == playback_slider_max_range)
                            {
                                passed_second = 0;
                                clearInterval(play_pause_timer);
                                playback_control_box_obj.ctrl({type:"adjust_play_button_state", data:{playback_state:"pause"}});
                                view_page_obj.ctrl({type:"pause_ipc", data:{inner_window_info:obj.inner_window_info}});
                                fdSliderController.increment("playback_slider", playback_slider_max_range);
                            }
                            else
                            {
                                fdSliderController.increment("playback_slider", next_time.offset_value - current_value);
                            }
                        }
                        else
                        {
                            passed_second = 0;
                            clearInterval(play_pause_timer);
                            playback_control_box_obj.ctrl({type:"adjust_play_button_state", data:{playback_state:"pause"}});
                            view_page_obj.ctrl({type:"pause_ipc", data:{inner_window_info:obj.inner_window_info}});
                        }
                    }, 1000);
                    //view_page_obj.ctrl({type:"play_ipc", data:{inner_window_info:obj.inner_window_info}});
                return 0;
            }
            else
            {
                return -1;
            }
        }
        function ccm_message_search(obj)
        {
            var node, node_sn, pnode, pnode_sn;

            playback_control_box_obj.ctrl({type:"remove_playback_slider_block"});

            event_array = new Array();

            node_sn = obj.inner_window_info? obj.inner_window_info.node_sn : g_ref.select_device_ipc;
            if(node_sn=="none")
             {
                 obj.inner_window_info=null;
                 node_sn=g_ref.select_device_ipc;
            }
            node = g_ref.device_list_obj["sn_" + node_sn];
            if(!node)
            {
                //log("Please select the specific device in ccm_message_search.");
                return;
            }
            if(node.isParent)
            {
                pnode = node;
                pnode_sn = node_sn;
            }
            else
            {
                pnode_sn = node.parent_sn;
                pnode = g_ref.device_list_obj["sn_" + pnode_sn];
            }

            obj.start_point = obj.start_point ? Date.parse(obj.start_point) / 1000 : "";
            obj.end_point = obj.end_point ? Date.parse(obj.end_point) / 1000 : "";

            acs.send_msg("ccm_msg_get", {sess:{nid:acs.create_nid(acs.ctrl({type:"get_info"})), sn:node_sn}, flag:0,
                start_time:obj.start_point, end_time:obj.end_point, filter:"record", start:(search_time), counts:get_event_count},
                function(type, msg, ref){ccm_message_search_ack(type, msg, ref);},
                {ip:pnode.ip, to:"ccm", sn:node_sn, psn:pnode_sn, start_point:obj.start_point, end_point:obj.end_point, inner_window_info:obj.inner_window_info || null});
        }
        function ccm_message_search_ack(type, msg, ref)
        {
            if(!me_parent) return;
            if(msg && msg.ret&& msg.ret.code == "")
            {
                if(!ref.inner_window_info)
                  playback_search_box_obj.ctrl({type:"create_playback_file_list", data:msg}); 
                if(ref.inner_window_info)
                        view_page_obj.ctrl({type:"playback_slider_is_ok", inner_window_info:ref.inner_window_info});
                if(msg.messages){
                     search_time=msg.messages[msg.messages.length-1].msg_id;
                }
            }
            else
            {
                // log("error:ccm_message_search_ack return Result error in class_playback_cntr.");
                return -1;
            }
        }
